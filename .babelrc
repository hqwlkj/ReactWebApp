{
  "presets": [
    "es2015",
    "react",
    "stage-1" //ES7 grammar support
  ],
  "plugins": [
    ["import", { "libraryName": "antd-mobile", "libraryDirectory": "lib", "style": false }],
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}
